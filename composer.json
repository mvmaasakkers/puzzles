{
  "name": "mvmaasakkers/puzzles",
  "type": "project",
  "authors": [
    {
      "name": "Martijn van Maasakkers",
      "email": "martijn@vanmaasakkers.net"
    }
  ],
  "require": {
    "php": ">=8.0",
    "symfony/flex": "^1.15",
    "symfony/asset": "^5.3",
    "symfony/twig-bundle": "^5.3",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "symfony/runtime": "^5.3",
    "symfony/yaml": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/dotenv": "^5.3",
    "symfony/maker-bundle": "^1.33"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
